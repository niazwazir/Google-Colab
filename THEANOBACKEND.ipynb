        "os.environ[\"KERAS_BACKEND\"] = \"theano\"\n",
        "import keras; import keras.backend\n",
        "if keras.backend.backend() != 'theano':\n",
        "    raise BaseException(\"This script uses other backend\")\n",
        "else:\n",
        "    keras.backend.set_image_dim_ordering('th')\n",
        "    print(\"Backend ok\")"
